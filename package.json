{
  "name": "test",
  "version": "1.0.0",
  "description": "Test Token",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "test": "mocha -r ts-node/register tests/**/*.spec.ts --reporter spec",
    "build": "generate-controller-interface -c TokenController -p ./token/tsconfig.json -o ../token-client",
    "cc:build": "tsc -p ./token/tsconfig.json",
    "cc:install": "node ./node_modules/.bin/chaincode-manager --config ./chaincode.config.json install token 1",
    "cc:upgrade": "node ./node_modules/.bin/chaincode-manager --config ./chaincode.config.json upgrade token",
    "cc:invoke": "node ./node_modules/.bin/chaincode-manager --config ./chaincode.config.json invoke token"
  },
  "dependencies": {
    "@worldsibu/convector-core-controller": "1.0.0-3",
    "@worldsibu/convector-core-model": "1.0.0-3",
    "reflect-metadata": "0.1.12"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "@types/node": "^10.3.2",
    "@types/uuid": "^3.4.3",
    "@worldsibu/convector-adapter-mock": "1.0.0-3",
    "@worldsibu/convector-tool-chaincode-manager": "1.0.0-3",
    "chai": "^4.1.2",
    "mocha": "^5.0.3",
    "rimraf": "^2.6.2",
    "ts-node": "^6.0.3",
    "typescript": "^2.8.3",
    "uuid": "^3.2.1"
  }
}
